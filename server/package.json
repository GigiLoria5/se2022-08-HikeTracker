{
    "dependencies": {
        "chai-http": "^4.3.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.0",
        "express-session": "^1.17.3",
        "express-validator": "^6.14.2",
        "file-type": "^18.0.0",
        "jest": "^29.2.1",
        "jest-sonar-reporter": "^2.0.0",
        "mocha-sonarqube-reporter": "^1.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "node-fetch": "^2.6.1",
        "nodemailer": "^6.8.0",
        "nodemon": "^2.0.20",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "sqlite3": "5.1.2"
    },
    "scripts": {
        "start": "nodemon index.js",
        "test": "jest",
        "test_coverage": "jest --coverage",
        "test_integration": "nyc mocha --runInBand --require mocha-steps --reporter mocha-sonarqube-reporter --timeout 30000 --exit",
        "test_integration_ma": "nyc mocha --runInBand --require mocha-steps --reporter mochawesome --timeout 30000 --exit",
        "test_format": "nyc report --reporter=lcov --report-dir=.nyc_coverage"
    },
    "devDependencies": {
        "chai": "^4.3.6",
        "mocha": "^10.1.0",
        "mocha-multi-reporters": "^1.5.1",
        "mocha-steps": "^1.3.0",
        "mochawesome": "^7.1.3",
        "nyc": "^15.1.0",
        "supertest": "^6.3.0"
    },
    "jest": {
        "testEnvironment": "node",
        "collectCoverage": true,
        "testResultsProcessor": "jest-sonar-reporter",
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/test/",
            "/coverage/",
            "/unit_test/",
            "/gpx_files/",
            "/.nyc_output/"
        ]
    },
    "jestSonar": {
        "reportPath": "reports"
    }
}
